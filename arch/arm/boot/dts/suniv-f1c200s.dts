&usb_otg {
	//dr_mode = "peripheral";
	dr_mode = "otg";
	status = "okay";
};

&usbphy {
	usb0_id_det-gpio = <&pio 4 10 GPIO_ACTIVE_HIGH>; /* PE10 */
	status = "okay";
};

&pwm {
	pinctrl-names = "default";
	pinctrl-0 = <&pwm1_pins>;
	status = "okay";
};
